# MultiversX Network Configuration
MVX_NETWORK=testnet
MVX_PROXY_URL=https://testnet-gateway.multiversx.com
MVX_API_URL=https://testnet-api.multiversx.com

# Smart Contract Address (set after deployment)
CONTRACT_ADDRESS=

# Wallet Private Key (for updating scores on-chain)
# WARNING: Keep this secret!
MVX_PRIVATE_KEY=

# AI Provider Configuration
# Get your free API key from: https://aistudio.google.com/app/apikey
AI_PROVIDER=google
GOOGLE_API_KEY=

# Optional: Portkey AI Gateway (alternative to direct Google API)
# PORTKEY_API_KEY=

# Optional: CoinGecko API Key (for higher rate limits)
# Get from: https://www.coingecko.com/en/api
COINGECKO_API_KEY=

# Server Configuration
PORT=3001
LOG_LEVEL=info
LOGS_DIR=./logs

# Node Environment
NODE_ENV=development
